{
  "name": "@volt-finance/stvol",
  "version": "1.0.0",
  "description": "volt finance stVol",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile": "npx hardhat compile",
    "deploy-eth:mainnet": "npx hardhat run --network mainnet scripts/deploy-eth-stvol.ts",
    "deploy-btc:mainnet": "npx hardhat run --network mainnet scripts/deploy-btc-stvol.ts",
    "deploy-eth:goerli": "npx hardhat run --network goerli scripts/deploy-eth-stvol.ts",
    "deploy-btc:goerli": "npx hardhat run --network goerli scripts/deploy-btc-stvol.ts",
    "deploy:arbitrum-goerli": "npx hardhat run --network arbitrum_goerli scripts/deploy.ts",
    "deploy:arbitrum": "npx hardhat run --network arbitrum scripts/deploy.ts",
    "test": "npx hardhat test",
    "test:iter": "hardhat test --network hardhat ./test/IteratableOrderSet.spec.ts",
    "test:v2": "hardhat test --network hardhat ./test/StVol.test.v2.ts",
    "test:coverage": "npx hardhat coverage",
    "test:size-contracts": "npx hardhat size-contracts",
    "verify:goerli": "npx hardhat verify --constructor-args dev.arguments.js --contract \"contracts/StVolUpDown.sol:StVolUpDown\" --network goerli 0x4e91f6EE7423E3E5531EFCc1FFB68F474D326452",
    "verify:mainnet": "npx hardhat verify --constructor-args prod.arguments.js --contract \"contracts/StVol1PerDown.sol:StVol1PerDown\" --network mainnet 0xF94e7F50120fe8276B85E21f31C6de097eab8813",
    "verify:arbitrum-goerli": "npx hardhat verify --constructor-args dev.arguments.js 0xa78E7c49BF3DD6d708aa439FF2B20a4458D40A4C --network arbitrum_goerli"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.2.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-truffle5": "^2.0.7",
    "@openzeppelin/contracts": "^4.2.0",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@types/mocha": "^10.0.1",
    "eslint": "^8.45.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.17.0",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.3",
    "solidity-coverage": "^0.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@pythnetwork/pyth-sdk-solidity": "^2.2.1",
    "@typechain/ethers-v5": "^11.1.1",
    "@typechain/hardhat": "^8.0.3",
    "dotenv": "^16.3.1",
    "prettier-plugin-solidity": "^1.1.3",
    "ts-generator": "^0.1.1",
    "typechain": "^8.3.1",
    "web3": "^1.10.3"
  }
}
