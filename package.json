{
  "name": "@volt-finance/stvol",
  "version": "1.0.0",
  "description": "volt finance stVol",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile": "npx hardhat compile",
    "deploy:goerli": "npx hardhat run --network goerli scripts/deploy-btc-stvol.ts",
    "deploy:sepolia": "npx hardhat run --network sepolia scripts/deploy-eth-stvol.ts",
    "deploy:base-sepolia": "npx hardhat run --network base_sepolia scripts/deploy-stvol-hourly.ts",
    "deploy:blast-sepolia": "npx hardhat run --network blast_sepolia scripts/deploy-blast-stvol.ts",
    "deploy:arbitrum-sepolia": "npx hardhat run --network arbitrum_sepolia scripts/deploy-eth-stvol.ts",
    "test": "npx hardhat test",
    "test:stvol-v1": "hardhat test --network hardhat ./test/StVol.v1.test.ts",
    "test:mock-pyth": "hardhat test --network hardhat ./test/MockPyth.test.ts",
    "verify:goerli": "npx hardhat verify --constructor-args dev.arguments.js --network goerli 0x3b66fb3aAF9F0A8cF9950b3b464cfdE002D07aae",
    "verify:sepolia": "npx hardhat verify --constructor-args dev.arguments.js --contract \"contracts/StVol0Per.sol:StVol0Per\" --network sepolia 0x0Ff5F73D50318f62a07cE1c7A4C124f5c5062AE9",
    "verify:base-sepolia": "npx hardhat verify --constructor-args base.dev.arguments.js --network base_sepolia 0x56b7F20033cD3909938B9953480706354a171582",
    "verify:arbitrum-sepolia": "npx hardhat verify --constructor-args arbi.dev.arguments.js --contract \"contracts/StVol0Per.sol:StVol0Per\" --network arbitrum_sepolia 0xA9A277401dA8C814052a18A0ccb5A2fC14BCb6DD",
    "verify:blast-sepolia": "npx hardhat verify --constructor-args blast.dev.arguments.js --network blast_sepolia 0x83c134e8952372c5efd34BbF8e87C3CA74D97247",
    "verify:arbitrum": "npx hardhat verify --constructor-args arbi.prod.arguments.js --contract \"contracts/StVolUpDown.sol:StVolUpDown\" --network arbitrum 0x40b0c000893164D6963597F572699107fB27CE04"
  },
  "devDependencies": {
    "@chainlink/contracts": "^1.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.5",
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/hardhat-upgrades": "^3.0.5",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/mocha": "^10.0.6",
    "eslint": "^8.57.0",
    "ethers": "^6.11.1",
    "hardhat": "^2.22.2",
    "husky": "^9.0.11",
    "solidity-coverage": "^0.8.11",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@inquirer/input": "^2.1.1",
    "@inquirer/select": "^2.2.1",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "@pythnetwork/pyth-sdk-solidity": "^2.4.1",
    "@redstone-finance/evm-connector": "^0.3.6",
    "dotenv": "^16.4.5",
    "prettier-plugin-solidity": "^1.3.1",
    "ts-generator": "^0.1.1",
    "web3": "^4.7.0"
  }
}
